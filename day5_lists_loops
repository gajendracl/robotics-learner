robot_parts = ['Sensors', 'Motor', 'Battery', 'Camera', 'Chip']
# Display robot parts
print(f" Robot Parts List: {robot_parts}")
robot_parts.append('AI_Module')

print(f" Updated Robot Parts List: {robot_parts}")

robot_parts.remove('Camera')
print(f" Updated Robot Parts List: {robot_parts}")

robot_parts[1] = 'Servo Motor'
print(f" Final Robot Parts List: ")

for index, part in enumerate(robot_parts):
    print(f"Position {index} : {part}")
